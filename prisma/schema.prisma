generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Users {
  id    String @id @default(uuid())
  name  String
  email String @unique

  snacks Snacks[]


  @@map("users")
}

model Snacks {
  id          String   @id @default(uuid())
  name        String
  description String
  date        DateTime @default(now())
  inDiet      Boolean

  user_id String
  user    Users  @relation(fields: [user_id], references: [id])

  @@map("snacks")
}
